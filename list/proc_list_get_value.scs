//Возвращает следующий элемент списка _next_element для элемента списка _current_element 

scp_program -> proc_list_get_value(*
	
	-> rrel_params: .proc_list_get_value_params (*
		-> rrel_1: rrel_in: _current_element;;
		-> rrel_2: rrel_out: _element_value;;
	*);;

	-> rrel_operators: .proc_list_get_value_operator_set (*
		
		-> rrel_init: -> .proc_list_get_value_operator1 (*
			<- varErase;;
			-> rrel_1: rrel_fixed: rrel_scp_var: _element_value;;

			=> nrel_goto: .proc_list_get_value_operator2;;
		*);;

		-> .proc_list_get_value_operator2 (*
			<- searchElStr5;;
