			-> rrel_assign: rrel_arc: rrel_pos: rrel_const: _arc2;;
			-> rrel_assign: _element_value;;
			-> rrel_assign: rrel_arc: rrel_pos: rrel_const: _arc4;;
			-> rrel_fixed:  rrel_scp_const: _list_value;;

			=> nrel_then: .proc_list_get_value_operator_return;;
			=> nrel_else: .proc_list_get_value_operator_return;;
		*);;

		-> .proc_list_get_value_operator_return (*
			<- return;;
		*);;	
	*);;
*);;